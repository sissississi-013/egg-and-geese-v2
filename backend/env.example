# Copy this file to .env and fill in your keys
# cp env.example .env

# === Database (PostgreSQL) ===
DATABASE_URL=postgresql+asyncpg://egggeese:egggeese2024@localhost:5432/egggeese

# === Neo4j AuraDB ===
NEO4J_URI=neo4j+s://YOUR_INSTANCE.databases.neo4j.io
NEO4J_USER=neo4j
NEO4J_PASSWORD=your-neo4j-password
NEO4J_DATABASE=neo4j

# === GLiNER / Fastino ===
# Mode: "local" loads model from HuggingFace
#        "fastino" calls Fastino's hosted /gliner-2 endpoint (recommended)
#        "pioneer" calls a self-deployed Pioneer endpoint
GLINER_MODE=fastino
GLINER_MODEL_ID=gliner-community/gliner_medium-v2.5
# Fastino hosted base URL (used when GLINER_MODE=fastino)
FASTINO_BASE_URL=https://api.fastino.ai
# Pioneer-deployed endpoint (only if GLINER_MODE=pioneer)
PIONEER_ENDPOINT_URL=
PIONEER_API_KEY=your-pioneer-api-key
# Entity confidence threshold (0-1)
GLINER_THRESHOLD=0.3

# === Yutori (Scouting API) ===
YUTORI_API_KEY=your-yutori-api-key
YUTORI_BASE_URL=https://api.yutori.com

# === Reka Vision ===
REKA_API_KEY=your-reka-api-key
REKA_BASE_URL=https://api.reka.ai

# === Senso ===
SENSO_API_KEY=your-senso-api-key
SENSO_BASE_URL=https://api.senso.ai/v1

# === Anthropic (Claude) ===
ANTHROPIC_API_KEY=your-anthropic-api-key

# === OpenClaw Gateway ===
OPENCLAW_GATEWAY_URL=http://localhost:3001

# === App ===
APP_ENV=development
METRICS_POLL_INTERVAL_MINUTES=30
